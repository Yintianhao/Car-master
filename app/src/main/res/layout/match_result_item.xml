<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="190dp">
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:orientation="vertical">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:divider="@drawable/divider_horizontal"
            android:showDividers="middle">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="用户名"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:id="@+id/match_userName"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="手机号"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:id="@+id/match_userPhone"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:divider="@drawable/divider_horizontal"
            android:showDividers="middle">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="车牌号"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:id="@+id/match_CarNum"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp">
            <TextView
                android:id="@+id/match_startToEnd"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:divider="@drawable/divider_horizontal"
            android:showDividers="middle">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="拼车前费用"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:id="@+id/match_moneyBefore"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:divider="@drawable/divider_horizontal"
            android:showDividers="middle">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="拼车后费用"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:id="@+id/match_moneyAfter"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:divider="@drawable/divider_horizontal"
            android:showDividers="middle">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="最早出发时间"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:id="@+id/match_TimeStart"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:divider="@drawable/divider_horizontal"
            android:showDividers="middle">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="最迟出发时间"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:id="@+id/match_TimeEnd"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:divider="@drawable/divider_horizontal"
            android:showDividers="middle">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="拼车前距离"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:id="@+id/match_DistanceBefore"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:divider="@drawable/divider_horizontal"
            android:showDividers="middle">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="拼车后距离"
                android:gravity="center"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
            <TextView
                android:id="@+id/match_DistanceAfter"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@android:style/TextAppearance.Small"
                android:background="@drawable/input_bg"/>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>