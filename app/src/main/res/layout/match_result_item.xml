    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/head"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/icon_driver"
        android:layout_margin="20dp"/>
    <TextView
        android:id="@+id/match_userName"
        android:text="名字"
        android:gravity="center"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:gravity="center"
        android:id="@+id/match_userPhone"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="13657433916"
        android:layout_marginRight="20dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp" />
    <RelativeLayout
        android:background="#808080"
        android:layout_marginTop="60dp"
        android:layout_width="match_parent"
        android:layout_height="5dp"></RelativeLayout>
    <TextView
        android:text="起点"
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="70dp"
        android:layout_marginLeft="20dp"/>
    <TextView
        android:id="@+id/match_start_place"
        android:gravity="center"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="70dp"
        android:layout_marginTop="70dp"/>
    <RelativeLayout
        android:background="#808080"
        android:layout_marginTop="110dp"
        android:layout_width="match_parent"
        android:layout_height="5dp"></RelativeLayout>
    <TextView
        android:gravity="center"
        android:text="终点"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="20dp"/>

    <TextView
        android:id="@+id/match_end_place"
        android:gravity="center"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="70dp"
        android:layout_marginTop="120dp"/>
    <RelativeLayout
        android:background="#808080"
        android:layout_marginTop="160dp"
        android:layout_width="match_parent"
        android:layout_height="5dp"></RelativeLayout>
    <TextView
        android:background="@drawable/icon_time"
        android:layout_marginTop="180dp"
        android:layout_marginLeft="20dp"
        android:layout_width="40dp"
        android:layout_height="40dp" />

    <TextView
        android:id="@+id/match_startTime_to_endTime"
        android:gravity="center"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="70dp"
        android:layout_marginTop="180dp"/>
